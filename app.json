{
  "expo": {
    "name": "B-TICKETS",
    "slug": "tickets-app",
    "scheme": "btickets",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-app.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/icon-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#3e0059",
      "imageWidth": 200
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.btickets.app",
      "buildNumber": "5",
      "infoPlist": {
        "NSCameraUsageDescription": "Cette application a besoin d'accéder à la caméra pour permettre la capture de photos de billets ou d'événements.",
        "NSPhotoLibraryUsageDescription": "Cette application a besoin d'accéder à votre photothèque pour permettre la sauvegarde ou la sélection de photos.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 2,
      "package": "com.btickets.app",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "READ_MEDIA_IMAGES"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon-app.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "description": "Un groupe qui œuvre dans le Marketing digital et vente de billets",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eab52e2d-f701-4b87-a2d3-5115a710a8ca"
      }
    }
  }
}
